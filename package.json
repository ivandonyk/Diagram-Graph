    "@types/node": "^14.14.25",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.10",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
